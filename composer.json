{
    "name": "CThomas87/scorm-cloud-laravel",
    "description": "Provides a ScormCloud API gateway for Laravel applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Asif Mohammed",
            "email": "asifm42@gmail.com",
            "role": "developer"
        }
    ],
    "keywords": [
        "asifm42",
        "scorm",
        "cloud",
        "laravel",
        "php"
    ],
    "require": {
        "php" : ">=7.0.0",
        "asifm42/scorm-cloud-php": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CThomas87\\ScormCloudGateway\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CThomas87\\ScormCloudGateway\\ScormCloudServiceProvider"
            ],
            "aliases": {
                "ScormCloud": "CThomas87\\ScormCloudGateway\\ScormCloud"
            }
        }
    }
}
